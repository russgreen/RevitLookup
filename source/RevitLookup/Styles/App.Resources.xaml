<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ui="http://lookupengine.com/xaml"
    xmlns:menu="clr-namespace:RevitLookup.UI.Framework.Markup.Menu;assembly=RevitLookup.UI.Framework"
    xmlns:membersGrid="clr-namespace:RevitLookup.Styles.ComponentStyles.MembersGrid">

    <membersGrid:DataGridCellTemplateSelector
        x:Key="DataGridCellTemplateSelector" />

    <ResourceDictionary.MergedDictionaries>

        <!-- Themes -->
        <ui:ThemesDictionary />
        <ui:ControlsDictionary />
        <menu:MenusDictionary />

        <!-- Controls -->
        <ResourceDictionary Source="pack://application:,,,/RevitLookup.UI.Framework;component/Controls/ContentPlaceholder/ContentPlaceholder.xaml" />

        <!-- Components Styles -->
        <ResourceDictionary Source="pack://application:,,,/RevitLookup;component/Styles/ComponentStyles/ObjectsTree/TreeGroupTemplates.xaml" />
        <ResourceDictionary Source="pack://application:,,,/RevitLookup;component/Styles/ComponentStyles/MembersGrid/DataGridGroupStyles.xaml" />
        <ResourceDictionary Source="pack://application:,,,/RevitLookup;component/Styles/ComponentStyles/MembersGrid/DataGridRowStyle.xaml" />
        <ResourceDictionary Source="pack://application:,,,/RevitLookup;component/Styles/ComponentStyles/MembersGrid/DataGridCellTemplate.xaml" />

    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>