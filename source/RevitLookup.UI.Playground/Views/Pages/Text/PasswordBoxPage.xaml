<Page
    x:Class="RevitLookup.UI.Playground.Views.Pages.Text.PasswordBoxPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://lookupengine.com/xaml"
    xmlns:controls="clr-namespace:RevitLookup.UI.Playground.Controls"
    mc:Ignorable="d"
    d:DesignHeight="450"
    d:DesignWidth="500"
    Foreground="{DynamicResource TextFillColorPrimaryBrush}">

    <StackPanel
        Margin="0,0,0,24">
        <controls:ControlExample
            Margin="0"
            HeaderText="WPF UI PasswordBox"
            XamlCode="&lt;ui:PasswordBox /&gt;">
            <ui:PasswordBox />
        </controls:ControlExample>

    </StackPanel>

</Page>